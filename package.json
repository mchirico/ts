{
  "name": "@mchirico/ts",
  "version": "1.0.9",
  "private": false,
  "description": "A Typescript Starter",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Mike Chirico (https://mchirico.github.io/) <mchirico@gmail.com>",
  "license": "Apache License 2.0",
  "repository": "https://github.com/mchirico/ts.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/mchirico"
  },
  "homepage": "https://github.com/mchirico/ts",
  "bugs": "https://github.com/mchirico/ts/issues",
  "keywords": [
    "ts",
    "github"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "node ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest --coverage && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "pub": "npm publish --access public"
  },
  "dependencies": {
    "@types/lodash": "^4.14.155",
    "@types/node-fetch": "^2.5.7",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.1.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.9.0",
    "nock": "^12.0.0",
    "nodemon": "^2.0.0",
    "standard": "^14.3.1",
    "ts-jest": "^25.1.0",
    "typescript": "^3.6.4"
  },
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
